#include<stdio.h>
#include<conio.h>
#include<math.h>
float area(float,float,float);
int main()
{
float a,b,c;
printf("Enter the sides of triangle");
scanf("%f %f %f",&a,&b,&c);
printf("The area of triangle=%f",area(a,b,c));
return 0;
}
float area(float x,float y,float z)
{
float s=(x+y+z)/2;
float m=sqrt(s*(s-x)*(s-y)*(s-z));
return m;
}
