#include<stdio.h>
int main()
{
char ch;
printf("\n Enter any character:");
scanf("%c",&ch);
switch(ch)
{
case'A':
case'a':
printf("\n%c is vowel",ch);
break;
case'E':
case'e':
printf("\n%c is vowel",ch);
break;
case'I':
case'i':
break;
case'O':
case'o':
printf("\n%c is vowel",ch);
break;
case'U':
case'u':
printf("\n%ch is vowel",ch);
break;
default:printf("%c is consonant",ch);
}
return 0;
}
